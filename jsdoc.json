{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules|utils|__tests__)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/jsdoc-template/",
    "encoding": "utf8",
    "destination": "gh-pages/docs/",
    "recurse": true,
    "verbose": true,
    "private": false
  },
  "templates": {
    "referenceTitle": "WanaKana",
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false
    }
  }
}
